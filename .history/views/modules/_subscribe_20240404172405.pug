section.subscribe
  .container.subscribe-container
    .subscribe-image
      each image [{src: "", alt: ""},]
        img(src= image.src, alt= image.alt)
    .subscribe-info
      h2.heading.subscribe-heading Subscribe For More Info and update from Houter
      .subscribe-now
        include ../icons/_icon-email
        input.subscribe-email(type="text" placeholder="Your email here")
        a.button.button-green-500.subscribe-button Subsribe Now

